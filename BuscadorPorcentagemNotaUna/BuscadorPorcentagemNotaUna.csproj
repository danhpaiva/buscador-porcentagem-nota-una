<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>ico\program.ico</ApplicationIcon>
    <Description>Calculadora Porcentagem - Daniel Paiva</Description>
    <Copyright>Daniel Paiva</Copyright>
    <PackageProjectUrl>https://github.com/danhpaiva/buscador-porcentagem-nota-una</PackageProjectUrl>
    <PackageIcon>program.png</PackageIcon>
    <RepositoryUrl>https://github.com/danhpaiva/buscador-porcentagem-nota-una</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Title>Buscador Porcentagem Nota UNA</Title>
    <PackageTags>note</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="ico\program.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ico\program.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
